      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "text": {
      "type": "blocks",
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "custom.slider",
        "custom.video-embed",
        "custom.video",
        "custom.rich-text"
      ]
    },
    "date": {
      "type": "date",
      "required": true
