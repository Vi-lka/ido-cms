{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Книги",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "text": {
      "type": "blocks"
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "order": {
      "type": "integer",
      "unique": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "custom.rich-text",
        "custom.files-list",
        "custom.slider",
        "custom.video",
        "custom.video-embed"
      ]
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "inversedBy": "books"
    }
  }
}
